<div class="box box-danger">
  <div class="box-header with-border">
    <h3 class="box-title"><%= t "courses.tab.trainers" %></h3>
  </div>
  <div class="box-body no-padding">
    <ul class="users-list clearfix">
      <%= render partial: "user_course", collection: @trainers, as: :user %>
    </ul>
  </div>
</div>

<div class="box box-primary">
  <div class="box-header with-border">
    <h3 class="box-title"><%= t "courses.tab.trainees" %></h3>
  </div>
  <div class="box-body no-padding">
    <ul class="users-list clearfix">
      <%= render partial: "user_course", collection: @trainees, as: :user %>
    </ul>
  </div>
</div>
