<div class="row">
  <div class="breadcrumb-bar">
    <ul class="list breadcrumb">
      <li>
        <%= link_to root_url do %>
          <i class="glyphicon glyphicon-th-list"></i>
          <%= t "layouts.paths" %>
        <% end %>
      </li>
      <li class="active"><%= t "all_report" %></li>
    </ul>
  </div>
</div>
<div class="row"><h3><%= t "my_report" %></h3></div>
<div class="row panel panel-default">
  <table class="table table-bordered table-hover">
    <tr>
      <th><%= t "reports.admin.tables.date" %></th>
      <th><%= t "reports.admin.tables.working_duration" %></th>
      <th><%= t "reports.admin.tables.line_code" %></th>
      <th><%= t "reports.admin.tables.created_at" %></th>
    </tr>
    <%= render @reports %>
  </table>
</div>
